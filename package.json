{
  "name": "nexus-ai-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace web dev\" \"yarn workspace api dev\"",
    "build": "yarn workspaces run build",
    "lint": "yarn workspaces run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "yarn workspaces run typecheck"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@1.22.19"
}
